{
  "name": "devaugment-extension",
  "displayName": "DevAugment",
  "publisher": "kjxcodez",
  "version": "0.1.0",
  "description": "DevAugment is an open-source AI-powered productivity extension for Visual Studio Code.\r It helps developers write better code faster by providing:",
  "categories": [
    "Other",
    "Productivity",
    "AI"
  ],
  "engines": {
    "vscode": "^1.106.0"
  },
  "activationEvents": [
    "onCommand:devaugment.reviewCode",
    "onCommand:devaugment.generateComments",
    "onCommand:devaugment.debugSuggestions",
    "onCommand:devaugment.setApiKey",
    "onCommand:devaugment.openSettings",
    "onView:devaugmentHistory"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "DevAugment",
      "properties": {
        "devaugment.provider": {
          "type": "string",
          "enum": [
            "openai",
            "gemini"
          ],
          "default": "gemini",
          "description": "Choose the AI provider."
        }
      }
    },
    "commands": [
      {
        "command": "devaugment.reviewCode",
        "title": "DevAugment: Review Code"
      },
      {
        "command": "devaugment.generateComments",
        "title": "DevAugment: Generate Comments"
      },
      {
        "command": "devaugment.debugSuggestions",
        "title": "DevAugment: Suggest Debug Steps"
      },
      {
        "command": "devaugment.setApiKey",
        "title": "DevAugment: Set API Key"
      },
      {
        "command": "devaugment.openSettings",
        "title": "DevAugment: Open Settings Panel"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevAugment/devaugment-extension.git"
  },
  "keywords": [],
  "author": {
    "name": "kjxcodez"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DevAugment/devaugment-extension/issues"
  },
  "homepage": "https://github.com/DevAugment/devaugment-extension#readme",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/node-fetch": "^2.6.13",
    "@types/vscode": "^1.106.1",
    "typescript": "^5.9.3",
    "vscode-test": "^1.6.1"
  }
}
